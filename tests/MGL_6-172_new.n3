    air:else [
        air:description ("Dissemination of " :INFO " is not limited by regulations of the board, as stated in MGL 6-172, Para. 5, Sent. 2, Cl. 3.");
	air:rule :MGL_6_172_Para_6_Sent_1_b_Cl_3 ].	# Check conflict of rules dig0018

#tbd0004
